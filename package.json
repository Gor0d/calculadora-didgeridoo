{
  "name": "didgemap",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:android:preview": "eas build --platform android --profile preview",
    "build:android:production": "eas build --platform android --profile production",
    "build:android:apk": "eas build --platform android --profile production-apk",
    "build:ios:preview": "eas build --platform ios --profile preview",
    "build:ios:production": "eas build --platform ios --profile production",
    "build:all:production": "eas build --platform all --profile production",
    "submit:android": "eas submit --platform android --profile production",
    "submit:ios": "eas submit --platform ios --profile production",
    "deploy:production": "npm run build:all:production && npm run submit:android && npm run submit:ios",
    "release": "node build-and-release.js",
    "prebuild": "expo prebuild --clean",
    "lint": "eslint src/ --ext .js,.jsx --fix",
    "lint:check": "eslint src/ --ext .js,.jsx",
    "format": "prettier --write \"src/**/*.{js,jsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,json,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "quality:check": "npm run lint:check && npm run format:check && npm run test:ci",
    "build:web": "expo export --platform web --output-dir build",
    "build:web:prod": "expo export --platform web --output-dir build --clear",
    "update:production": "eas update --branch production --message \"Production update\"",
    "update:preview": "eas update --branch preview --message \"Preview update\"",
    "build:local:android": "eas build --platform android --profile preview --local",
    "build:quick": "eas build --platform android --profile preview",
    "deploy:web": "npm run build:web:prod",
    "setup:eas": "eas build:configure",
    "serve": "node simple-server.js",
    "deploy": "npm run build:web && npm run serve",
    "prebuild:check": "node pre-build-check.js",
    "prebuild:full": "npm run prebuild:check && npm run test && npm run lint:check"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-navigation/bottom-tabs": "^7.4.2",
    "@react-navigation/native": "^7.1.14",
    "@react-navigation/native-stack": "^7.3.21",
    "@react-navigation/stack": "^7.4.5",
    "@reduxjs/toolkit": "^2.8.2",
    "@sentry/react-native": "~6.14.0",
    "expo": "^53.0.20",
    "expo-av": "~15.1.7",
    "expo-blur": "~14.1.5",
    "expo-dev-client": "^5.2.4",
    "expo-document-picker": "~13.1.6",
    "expo-file-system": "~18.1.11",
    "expo-haptics": "~14.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-sharing": "~13.1.5",
    "expo-status-bar": "~2.2.3",
    "expo-updates": "^0.28.17",
    "express": "^5.1.0",
    "html2canvas": "^1.4.1",
    "html2pdf.js": "^0.10.3",
    "jspdf": "^3.0.1",
    "prop-types": "^15.8.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "^15.11.2",
    "react-native-web": "^0.20.0",
    "react-redux": "^9.2.0"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-status-bar-height"
        ]
      }
    }
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-transform-export-namespace-from": "^7.27.1",
    "@testing-library/react-native": "^12.7.2",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-native": "^4.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-expo": "~53.0.9",
    "prettier": "^3.2.5",
    "react-test-renderer": "^19.0.0",
    "typescript": "~5.8.3"
  },
  "resolutions": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "overrides": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-test-renderer": "^19.0.0"
  },
  "private": true
}
