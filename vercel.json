{
  "name": "didgemap-calculadora",
  "version": 2,
  "buildCommand": "npm run build:web:prod",
  "outputDirectory": "build",
  "installCommand": "npm ci",
  "framework": null,
  "cleanUrls": true,
  "trailingSlash": false,
  "public": false,
  "github": {
    "enabled": true,
    "autoAlias": true
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/index.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}